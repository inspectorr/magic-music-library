{
  "name": "magic-music-library-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:local": "cross-env NODE_ENV=development node server.js",
    "dev:remote": "cross-env NODE_ENV=development NEXT_PUBLIC_USE_REMOTE_API=true node server.js",
    "build": "cross-env NODE_ENV=production NEXT_PUBLIC_USE_REMOTE_API=true next build",
    "start": "cross-env NODE_ENV=production NEXT_PUBLIC_USE_REMOTE_API=true node server.js -p $PORT"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "dotenv": "^8.5.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.3.1",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.4.0",
    "sass": "^1.32.12"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
